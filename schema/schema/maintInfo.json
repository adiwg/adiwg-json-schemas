{
  "id": "maintInfo.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "schema for maintenance information",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "maintenanceFrequency": {
      "type": "string"
    },
    "maintenanceNote": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
    ,
    "maintenanceContact": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "$ref": "contact.json#/definitions/contactRef"
      }
    }
  }
}