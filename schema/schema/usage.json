{
  "id": "usage.json#",
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "schema for usage",
  "type": "object",
  "required": ["specificUsage", "userContactInfo"],
  "additionalProperties": false,
  "properties": {
    "specificUsage": {
      "type": "string"
    },
    "userDeterminedLimitation": {
      "type": "string"
    },
    "userContactInfo": {
      "type": "array",
      "uniqueItems": true,
      "items": {
        "$ref": "contact.json#/definitions/contactRef"
      }
    }
  }
}